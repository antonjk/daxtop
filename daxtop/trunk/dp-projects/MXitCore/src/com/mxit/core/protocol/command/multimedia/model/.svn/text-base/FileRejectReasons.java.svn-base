package com.mxit.core.protocol.command.multimedia.model;

/**
 * @author Dax Frost
 */
public class FileRejectReasons
{
    /** Rejected by user */
    public static final byte UserRejected = 1;

    /** Unsupported mime type */
    public static final byte UnsupportedFile = 2;

    /** Not enough resources available for this file */
    public static final byte NotEnoughResourcesAvailable = 3;

    /** Invalid recipient (contact not found) */
    public static final byte InvalidRecipient = 4;
}
