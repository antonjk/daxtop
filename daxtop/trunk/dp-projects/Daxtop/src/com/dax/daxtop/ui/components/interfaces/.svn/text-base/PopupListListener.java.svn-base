package com.dax.daxtop.ui.components.interfaces;

/**
 * @author Dax Booysen
 *
 * A listener that will invoke calls when list was used
 */
public interface PopupListListener
{
    /** A list item was chosen */
    abstract void PopupListItemChosen(Object item);

    /** A list item was chosen */
    abstract void PopupListCancelled();
}
