application.desc=daxtop MXit social client developed by Dax Booysen.
application.homepage=www.daxtris.com
application.title=daxtop MXit Client
application.vendor=daxtris
build.classes.dir=${build.dir}/classes
build.classes.excludes=**/*.java,**/*.form
# This directory is removed when the project is cleaned:
build.dir=build
build.generated.dir=${build.dir}/generated
build.generated.sources.dir=${build.dir}/generated-sources
# Only compile against the classpath explicitly listed here:
build.sysclasspath=ignore
build.test.classes.dir=${build.dir}/test/classes
build.test.results.dir=${build.dir}/test/results
debug.classpath=\
    ${run.classpath}
debug.test.classpath=\
    ${run.test.classpath}
# This directory is removed when the project is cleaned:
dist.dir=dist
dist.jar=${dist.dir}/Daxtop.jar
dist.javadoc.dir=${dist.dir}/javadoc
excludes=
file.reference.jcalendar-1.3.3.jar=../libs/jcalendar/jcalendar-1.3.3.jar
file.reference.substance.jar=../libs/substance/6.0/substance.jar
file.reference.trident.jar=../libs/substance/6.0/trident.jar
includes=**
jar.compress=true
javac.classpath=\
    ${reference.DaxChatPane.jar}:\
    ${reference.DaxLib.jar}:\
    ${reference.DaxList.jar}:\
    ${file.reference.jcalendar-1.3.3.jar}:\
    ${file.reference.substance.jar}:\
    ${file.reference.trident.jar}
# Space-separated list of extra javac options
javac.compilerargs=
javac.deprecation=false
javac.source=1.5
javac.target=1.5
javac.test.classpath=\
    ${javac.classpath}:\
    ${build.classes.dir}:\
    ${libs.junit.classpath}:\
    ${libs.junit_4.classpath}
javadoc.additionalparam=
javadoc.author=false
javadoc.encoding=${source.encoding}
javadoc.noindex=false
javadoc.nonavbar=false
javadoc.notree=false
javadoc.private=false
javadoc.splitindex=true
javadoc.use=true
javadoc.version=false
javadoc.windowtitle=
jnlp.codebase.type=local
jnlp.codebase.url=file:/C:/Documents%20and%20Settings/Dax%20Booysen/My%20Documents/NetBeansProjects/DAXTOP/Daxtop/dist/
jnlp.enabled=false
jnlp.offline-allowed=false
jnlp.signed=false
main.class=com.dax.Main
manifest.file=manifest.mf
meta.inf.dir=${src.dir}/META-INF
platform.active=default_platform
project.DaxChatPane=../DaxChatPane
project.DaxLib=../DaxLib
project.DaxList=../DaxList
reference.DaxChatPane.jar=${project.DaxChatPane}/dist/DaxChatPane.jar
reference.DaxLib.jar=${project.DaxLib}/dist/DaxLib.jar
reference.DaxList.jar=${project.DaxList}/dist/DaxList.jar
run.classpath=\
    ${javac.classpath}:\
    ${build.classes.dir}
run.jvmargs=-Xmx1024M -splash:splash.png
run.test.classpath=\
    ${javac.test.classpath}:\
    ${build.test.classes.dir}
source.encoding=UTF-8
src.dir=src
test.src.dir=test
